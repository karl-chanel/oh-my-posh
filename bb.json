{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",

  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [

        {
          "type": "session",
          "style": "diamond",
          "trailing_diamond": "\uE0B4 ",
          "leading_diamond": "\uE0B6",
          "foreground": "#9B6BDF",
          "background": "#424242",
          "properties": {
            "display_host": true,
            "postfix": " \ue711",
            "prefix": ""
          }
        }
      ,
        {
          "type": "time",
          "style": "diamond",
          "trailing_diamond": "\uE0B4 ",
          "leading_diamond": "\uE0B6",
          "foreground": "#FFBB00",
          "background": "#424242",
          "properties": {
            "time_format": "15:04:05",
            "prefix": "",
            "postfix": ""
          }
        },
        {
          "type": "path",
          "style": "diamond",
          "trailing_diamond": "\uE0B4",
          "leading_diamond": "\uE0B6",
          "foreground": "#3EC669",
          "background": "#424242",
          "properties": {
            "prefix": "\uE5FF ",
            "postfix": "",
            "style": "agnoster_short",
            "max_depth": 2,
            "folder_separator_icon": "/"
          }
        },
        {
          "type": "git",
          "style": "diamond",
          "trailing_diamond": "\uE0B4",
          "leading_diamond": " \uE0B6",
          "foreground": "#43CCEA",
          "background": "#424242",
          "properties": {
            "prefix": "",
            "postfix": "",
            "display_status": true,
            "display_branch_status": true,
            "display_status_detail": true,
            "display_upstream_icon": true,
            "display_stash_count": true
          }
        },
        {
          "type": "python",
          "style": "diamond",
          "trailing_diamond": "\uE0B4",
          "leading_diamond": " \uE0B6",
          "foreground": "#E4F34A",
          "background": "#424242",
          "properties": {
            "prefix": "\uE235",
            "postfix": "",
            "display_version": false
          }
        },
        {
          "type": "go",
          "style": "diamond",
          "trailing_diamond": "\uE0B4",
          "leading_diamond": " \uE0B6",
          "foreground": "#7FD5EA",
          "background": "#424242",
          "properties": {
            "prefix": "\uFCD1",
            "postfix": "",
            "display_version": true
          }
        },
        {
          "type": "node",
          "style": "diamond",
          "trailing_diamond": "\uE0B4",
          "leading_diamond": " \uE0B6",
          "foreground": "#42E66C",
          "background": "#424242",
          "properties": {
            "prefix": "\uE718",
            "postfix": "",
            "display_version": false
          }
        },
        {
          "type": "ruby",
          "style": "diamond",
          "trailing_diamond": "\uE0B4",
          "leading_diamond": " \uE0B6",
          "foreground": "#E64747",
          "background": "#424242",
          "properties": {
            "prefix": "\uE791",
            "postfix": "",
            "display_version": false
          }
        },
        {
          "type": "java",
          "style": "diamond",
          "trailing_diamond": "\uE0B4",
          "leading_diamond": " \uE0B6",
          "foreground": "#E64747",
          "background": "#424242",
          "properties": {
            "prefix": "\uE738",
            "postfix": "",
            "display_version": true
          }
        },
        {
          "type": "julia",
          "style": "diamond",
          "trailing_diamond": "\uE0B4",
          "leading_diamond": " \uE0B6",
          "foreground": "#9B6BDF",
          "background": "#424242",
          "properties": {
            "prefix": "\uE624",
            "postfix": "",
            "display_version": false
          }
        },
        {
          "type": "battery",
          "style": "diamond",
          "trailing_diamond": "\uE0B4",
          "leading_diamond": " \uE0B6",
          "foreground": "#9B6BDF",
          "background": "#424242",
          "properties": {
            "battery_icon": "",
            "discharging_icon": "\u21E3 ",
            "charging_icon": "\u21E1 ",
            "charged_icon": "\u25CF ",
            "color_background": false,
            "charging_color": "#09fc11",
            "charged_color": "#40c4ff",
            "discharging_color": "#ff5722",
            "prefix": "",
            "postfix": "",
            "display_charging": true
          }
        }
      ,
        {
          "type": "executiontime",
          "style":"diamond",
          "leading_diamond": " \uE0B6",
          "foreground": "#ac730a",
          "background": "#424242",

          "properties": {
            "always_enabled": true,
            "prefix": "",
            "postfix": ""
          }
        },
        {
          "type": "memory",
          "style": "powerline",
          "foreground": "#26C6DA",
          "background": "#424242",
          "properties": {
            "precision": 1,
            "prefix": " \uf85a ",
            "postfix": "% "
          }
        },
        {
          "type": "exit",
          "style": "diamond",
          "foreground": "#038138",
          "background": "#424242",
          "trailing_diamond": "\uE0B4",
          "properties": {
            "display_exit_code": false,
            "always_enabled": true,
            "error_color": "#f1184c",

            "prefix": "îˆº",
            "postfix": ""
          }
        } ,
        {
          "type": "shell",
          "style": "diamond",
          "trailing_diamond": "\uE0B4",
          "leading_diamond": " \uE0B6",
          "foreground": "#ffffff",
          "background": "#0d5783",
          "properties": {
            "postfix": "",
            "prefix": ""
          }
        }


      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "text",
          "style": "plain",
          "foreground": "#f1184c",
          "properties": {
            "prefix": "",
            "text": "\u0023",
            "postfix": ""
          }
        }
      ]
    }
  ],
  "final_space": true,
  "console_title": true,
  "console_title_style": "template",
  "console_title_template": "{{ .Shell }} in {{ .Folder }}"

}
